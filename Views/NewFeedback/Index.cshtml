@model Service.Email.Model.NewFeedback
@{

}

<tr>
    <td>
        <h3 style="color:#259e66">Olá @(Model.nomeUsuario),</h3>
    </td>
</tr>
<tr>
    <td>
        <p>
            Obrigado pelo seu feedback.
            <br />
            A sua mensagem foi encaminhada ao setor responsável e entraremos em contato o mais rápido possível.
        </p>
        <p>Veja a baixo a mensagem enviada:</p>
    </td>
</tr>

<tr>
    <td style="border-radius: 7px; padding: 15px;">
        <h3 style="text-align:center;">@Model.dataHoraInclusao.Value.Day de @Model.dataHoraInclusao.Value.ToString("MMMM", new System.Globalization.CultureInfo("pt-BR")) de @Model.dataHoraInclusao.Value.Year, @Model.dataHoraInclusao.Value.ToString("HH:mm")</h3>
        <p>@Model.assunto</p>
        <p>@Model.mensagem</p>
    </td>
</tr>

<tr>
    <td>
        <p style="font-weight:600; text-align:center;">
            Atenciosamente,
            <br />
            Equipe X.
        </p>
    </td>
</tr>
